    <hal format="aidl" optional="true">
    <name>android.hardware.ir</name>
    <version>1</version>
    <interface>
        <name>IConsumerIr</name>
        <instance>default</instance>
    </interface>
    </hal>
    <hal format="aidl" optional="true">
    <name>android.hardware.sensors</name>
    <version>2</version>
    <interface>
        <name>ISensors</name>
        <instance>default</instance>
    </interface>
